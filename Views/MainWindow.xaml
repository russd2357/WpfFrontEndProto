<Window x:Class="WpfFrontEndProto.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfFrontEndProto.Views"
        mc:Ignorable="d"
        Title="View Data" Height="200" Width="400">
    <Grid Width="350">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Width="Auto">
            <Button Width="75" Height="75" Command="{Binding ViewDataCommand}" ToolTip="View Data">
                <Image Source="/Images/view_icon.png"></Image>
            </Button>
            <Button Width="75" Height="75" Margin="10,0,0,0" Command="{Binding SeedDataCommand}" ToolTip="Seed Data">
                <Image Source="/Images/seed_icon.png"></Image>
            </Button>
        </StackPanel>
    </Grid>
</Window>
